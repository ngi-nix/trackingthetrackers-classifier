{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://example.com/object1592822226.json",
	"title": "Root",
	"type": "object",
	"required": [
		"meta",
		"apks"
	],
	"properties": {
		"meta": {
			"$id": "#root/meta",
			"title": "Meta",
			"type": "object",
			"required": [
				"ver"
			],
			"properties": {
				"ver": {
					"$id": "#root/meta/ver",
					"title": "Ver",
					"type": "string",
					"default": "",
					"examples": [
						"0.1.0"
					],
					"pattern": "^.*$"
				}
			}
		}
,
		"apks": {
			"$id": "#root/apks",
			"title": "Apks",
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/apks/items",
				"title": "Items",
				"type": "object",
				"required": [
					"applicationId",
					"sha256",
					"permissions",
					"domainNames",
					"dependencies",
					"apiKeyIds"
				],
				"properties": {
					"applicationId": {
						"$id": "#root/apks/items/applicationId",
						"title": "applicationId",
						"type": "string",
						"default": "",
						"examples": [
							"video.like"
						],
						"pattern": "^.*$"
					},
					"sha256": {
						"$id": "#root/apks/items/sha256",
						"title": "SHA-256 checksum",
						"type": "string",
						"default": "",
						"examples": [
							"15D5E111799831F52ADE602B1C46936B0B1828F2"
						],
						"pattern": "^.*$"
					},
					"permissions": {
						"$id": "#root/apks/items/permissions",
						"title": "Permissions",
						"type": "array",
						"default": []
					},
					"domainNames": {
						"$id": "#root/apks/items/domainNames",
						"title": "Domain Names",
						"type": "array",
						"default": [],
						"items":{
							"$id": "#root/apks/items/domainNames/items",
							"title": "Items",
							"type": "string",
							"default": "",
							"examples": [
								"google.com"
							],
							"pattern": "^.*$"
						}
					},
					"dependencies": {
						"$id": "#root/apks/items/dependencies",
						"title": "Maven/Gradle Dependencies",
						"type": "array",
						"default": [],
						"items":{
							"$id": "#root/apks/items/dependencies/items",
							"title": "Items",
							"type": "string",
							"default": "",
							"examples": [
								"com.video.like.foo"
							],
							"pattern": "^.*$"
						}
					},
					"apiKeyIds": {
						"$id": "#root/apks/items/apiKeyIds",
						"title": "Identifiers for including API Keys",
						"type": "array",
						"default": [],
						"items":{
							"$id": "#root/apks/items/apiKeyIds/items",
							"title": "Items",
							"type": "string",
							"default": "",
							"examples": [
								"9226C22D11C13156C4F9E2DBD5FFB57BF3D5DC2F"
							],
							"pattern": "^.*$"
						}
					}
				}
			}

		}
	}
}
